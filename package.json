{
  "name": "x-grpc-server",
  "version": "1.0.0",
  "description": "基于x-grpc的NODE分布式服务",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "compose-up": "docker-compose -f docker-compose.yml up -d --build",
    "compose-down": "docker-compose -f docker-compose.yml down",
    "stack-deploy": "docker stack deploy --prune -c docker-compose.yml n1",
    "stack-rm": "docker stack rm n1 && && docker service rm n1_rabbitmq && docker network rm n1_overlay"
  },
  "author": "cheneyxu",
  "license": "ISC",
  "dependencies": {
    "amqplib": "^0.5.3",
    "config": "^3.0.1",
    "x-grpc": "^0.9.1"
  }
}